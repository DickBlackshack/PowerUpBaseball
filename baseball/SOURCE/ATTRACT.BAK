PRINC:	macro	string
	#ifndef	PrintCentered
	.global	PrintCentered
	#endif
	move.l	#striii\@,a0
	clr.w	d6	
	move.w	#$7FFF,d7
	jsr	PrintCentered	;entry point for plot print
	bra	dprrr\@
striii\@:
	dc.b	string,0
dprrr\@:	ds	0	;align prg counter
	endm

