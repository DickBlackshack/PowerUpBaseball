BlitWinList
	IMAGES	#Title
	APALETTE	#P_TITLERB
	BLIT	#pinrb,#0,#0

	SETCLIP	#0,#50,#384,#256

	IMAGES	#Misc
	APALETTE	#P_NEWSCORE
	move.w	XPOSITION(a5),d6
	move.w	YPOSITION(a5),d7
	add.w	#0,d7
	BLIT	#top10num,d6,d7

;	APALETTE	#P_FONTY2
;	APALETTE	#P_NEWSCORE
;	SETPRINT	#16,#24,#fnt2,#0
;	SETPRINT	#20,#34,#neo2,#0
	clr.l	FontAdjust
;	move.l	#neo2Spacing,FontSpacing
;	clr.l	FontSpacing	;test
;	move.l	#F2CharTrans,CharTrans ; character translation table
;	move.l	#SysCharTrans,CharTrans	; character translation table
	move.l	#NeoCharTrans,CharTrans ; character translation table


	move.w	XPOSITION(a5),d6
	add.w	#55,d6
	move.w	d6,ListX
	move.w	YPOSITION(a5),d7
	add.w	#48,d7
	move.w	d7,ListY

	move.l	T10list,ListPtr
	move.w	#10,ListCnt
.listlp
	SETPRINT	#20,#34,#neo2,#0
	move.l	#neo2Spacing,FontSpacing
	APALETTE	#P_NEWSCORE
	PLOT	ListX,ListY
	move.l	([ListPtr]),a0
	clr.w	d6
	move.w	#3,d7	;print 3 letters

	move.l	a0,a1
	jsr	AsciiTest
	bne	.noprint
	adda.l	#1,a1
	jsr	AsciiTest
	bne	.noprint
	adda.l	#1,a1
	jsr	AsciiTest
	bne	.noprint

	jsr	Print

;	move.w	ListX,d6
;	add.w	#80,d6
;	PLOT	d6,ListY

	SETPRINT	#20,#34,#neo2,#0
	clr.l	FontSpacing
	PLOT	#180,ListY

	move.l	([ListPtr]),a0
	move.w	T10data,d0
	tst.w	(a0,d0)
	beq.s	.no0
	clr.l	FontSpacing
	APALETTE	#P_NEWSCORE
	PRINTDEC	<(a0,d0.w)>,#4,JRGTS	;print stat
;trophy
	move.l	([ListPtr]),a0
	move.w	#REC_BeatGame,d0
	tst.w	(a0,d0.w)
	beq.s	.notrophy
	APALETTE	#P_TROPHY
	move.w	XPOSITION(a5),d0
	sub.w	#24,d0	;offset from list
	move.w	ListY,d1
	sub.w	#4,d1	;offset from list
	BLIT	#trophysm,d0,d1
.notrophy

.no0
.noprint
	add.w	#38,ListY	;vert spacing
	add.l	#4,ListPtr	;next list entry
	sub.w	#1,ListCnt
	bne	.listlp
	SETCLIP	#0,#0,#384,#256
	rts

WinProc
	cmp.w	#15,YPOSITION(a5)
	bge.s	.rts
	add.w	#2,YPOSITION(a5)
.rts	rts


AsciiTest
	cmp.b	#' ',(a1)
	beq.s	.ok
	cmp.b	#'A',(a1)
	blt.s	.notok
	cmp.b	#'Z',(a1)
	bgt.s	.notok

.ok	move.b	#0,d1
	rts

.notok	move.b	#1,d1
	rts

